# Configuration Example for Tyna Meet
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Backend URL (for WebSocket connection)
# Development: http://localhost:3001
# Production: https://your-backend-domain.com
VITE_BACKEND_URL=http://localhost:3001

# Backend API Keys (for server)
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here
DEEPGRAM_API_KEY=your_deepgram_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (for chat and summaries)
OPENAI_API_KEY=sk-proj-your_openai_api_key_here
VITE_OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# Gemini API Key (optional, for AI features)
GEMINI_API_KEY=your_gemini_api_key_here

# Google Calendar OAuth Configuration
# Get credentials from: https://console.cloud.google.com/
# 1. Create OAuth 2.0 Client ID
# 2. Add authorized redirect URI: https://your-backend-domain.com/api/auth/callback/google
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-your_google_client_secret
GOOGLE_REDIRECT_URI=https://your-backend-domain.com/api/auth/callback/google
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Node Environment
NODE_ENV=development

# Port (for backend server)
PORT=3001

# Frontend URL (for CORS in production)
FRONTEND_URL=http://localhost:3000

